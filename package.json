{
    "name": "ng-typeview",
    "version": "0.0.22",
    "description": "library to enable type-checking of angular views when using typescript",
    "keywords": [
        "angularjs",
        "angular",
        "typescript"
    ],
    "homepage": "https://github.com/emmanueltouzery/ng-typeview",
    "main": "dist/src/index.js",
    "typings": "dist/src/index.d.ts",
    "license": "MIT",
    "author": "Emmanuel Touzery",
    "contributors": [],
    "repository": {
        "type": "git",
        "url": "git://github.com/emmanueltouzery/ng-typeview.git"
    },
    "bugs": {
        "url": "https://github.com/emmanueltouzery/ng-typeview/issues"
    },
    "dependencies": {
        "@types/glob": "5.0.30",
        "@types/htmlparser2": "3.7.29",
        "@types/mocha": "2.2.40",
        "@types/node": "0.0.2",
        "glob": "7.1.1",
        "htmlparser2": "3.9.2",
        "immutable": "https://github.com/facebook/immutable-js/tarball/d66e3fe13830df8ab5ecc867a3ff0a2384e2621d",
        "monet": "https://github.com/cwmyers/monet.js/tarball/41b8ede0aeb6d4de73bed1163608ead4a900ea7b",
        "typescript": "2.3.2",
        "parsimmon": "1.2.0",
        "@types/parsimmon": "1.0.1"
    },
    "devDependencies": {
        "mocha": "3.2.0",
        "typedoc": "0.5.7"
    },
    "scripts": {
        "test": "tsc && ./node_modules/mocha/bin/mocha ./dist/test/*.js",
        "clean": "rm -Rf ./dist",
        "docgen": "./node_modules/typedoc/bin/typedoc --mode file --out apidoc --excludePrivate --excludeExternals --excludeNotExported --ignoreCompilerErrors src/index.ts"
    }
}
